package ch.pschatzmann.jflightcontroller4pi.parameters;

/*
 * All supported parameters need to be indentified by this enum which is used as key to look
 * up the value in the ParameterStore
 */
public enum ParametersEnum {
	ROLL, PITCH, YAW, THROTTLE, // standardized input controls -1.0 >= x <= 1.0
	SENSORROLL, SENSORPITCH, SENSORYAW, SENSORSPEED, SENSORDIRECTION, SENSORALTITUDE, // Values from Sensors
	MODE, RUDDER, AILERON, ELEVETOR, SPEED,
	GYROX, GYROY, GYROZ, 
	ACCELEROMETERX, ACCELEROMETERY, ACCELEROMETERZ, 
	MAGNETOMETERX, MAGNETOMETERY, MAGNETOMETERZ,
	// targets from Autopilot
	AUTODIRECTION, AUTOALTITUDE;
}
